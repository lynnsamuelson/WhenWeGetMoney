@model IEnumerable<WhenWeGetMoney.Models.Wish>

@{
    Layout = "~/Views/Shared/_WhenWeGetMoney.cshtml";
}


<h1> When We Get Money</h1>
<div class="col-xs-12">
    <input type="text" placeholder="search" />
</div>
<div>
    <h3>Wish List</h3>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="wish">
            <table>
                <th>
                    <td>Priotiy</td>
                    <td>Wish</td>
                    <td>Date Added</td>
                </th>
                
                    @foreach (var Wish in Model)
                            {
                                <tr>
                                    <td>@Wish.WishPriority;</td>
                                    <td>@Wish.Content;</td>
                                    <td>@Wish.Date;</td>
                                </tr>
                            }
              </table>
            </div>
        </div>
    </div>  

   